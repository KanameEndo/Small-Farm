<h1>プロフィール編集</h1>
<table>
  <%= form_with model: @user do |form| %>
    <tr>
      <td>ユーザー名
      <td><%= form.text_field :name %></td>
    </tr>
    <tr>
      <td>メールアドレス
      <td><%= form.email_field :email %></td>
    </tr>
    <tr>
      <td><%= form.submit "保存" %></td>
    </tr>
  <% end %>
<table>